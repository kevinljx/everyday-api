{
  "name": "Invoice",
  "plural": "invoices",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "mixins": {
    "Reminder": true
  },
  "scope": {
    "include": [
      "notes"
    ]
  },
  "properties": {
    "date": {
      "type": "date"
    },
    "latest": {
      "type": "boolean",
      "default": true
    },
    "state": {
      "type": "string",
      "default": "Draft"
    },
    "sent_date": {
      "type": "date",
      "default": "$now"
    },
    "quoteID": {
      "type": "string"
    },
    "currency": {
      "type": "object"
    },
    "currency_rate": {
      "type": "number"
    },
    "version": {
      "type": "number",
      "default": 1
    },
    "subtotal": {
      "type": "number"
    },
    "tax_amount": {
      "type": "number"
    },
    "discount_total": {
      "type": "number"
    },
    "reconcile": {
      "type": "boolean",
      "default": false
    },
    "tnc": {
      "type": "string"
    },
    "attn_toId": {
      "type": "string"
    },
    "totalAmt": {
      "type": "number"
    },
    "discount_rate": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "shipping": {
      "type": "string"
    },
    "billing": {
      "type": "string"
    },
    "sentOn": {
      "type": "date"
    },
    "dueDate": {
      "type": "date"
    },
    "account": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "id"
    },
    "attn_toId": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "id"
    },
    "ship_to": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "id"
    },
    "tax": {
      "type": "belongsTo",
      "model": "Tax",
      "foreignKey": "id"
    },
    "discount": {
      "type": "belongsTo",
      "model": "DiscountSetting",
      "foreignKey": ""
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    }
  },
  "acls": [],
  "methods": {}
}
