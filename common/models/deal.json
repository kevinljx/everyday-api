{
  "name": "Deal",
  "plural": "deals",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "customerInfo": "showCustomerInfo",
        "accountInfo": "showAccountInfo"
      }
    },
    "ShowUsers": true,
    "DeleteHistory": true
  },
  "scope": {
    "include": ["stage", "type", "events", "notes", "source", "history"]
  },
  "properties": {
    "closingDate": {
      "type": "date",
      "required": true
    },
    "amount": {
      "type": "number",
      "required": true
    },
    "info": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "history": {
      "type": "hasMany",
      "model": "DealHistory"
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    },
    "source": {
      "type": "belongsTo",
      "model": "LeadSource",
      "foreignKey": ""
    },
    "type": {
      "type": "belongsTo",
      "model": "DealType",
      "foreignKey": ""
    },
    "stage": {
      "type": "belongsTo",
      "model": "DealStage",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
