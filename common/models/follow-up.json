{
  "name": "FollowUp",
  "plural": "followups",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "DeleteHistory": true
  },
  "scope": {
    "include": ["type", "result", "reminders"]
  },

  "properties": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "followupable": {
      "type": "belongsTo",
      "polymorphic": true
    },
    "type": {
      "type": "belongsTo",
      "model": "FollowUpType"
    },
    "result": {
      "type": "belongsTo",
      "model": "FollowUpResult"
    },
    "reminders": {
      "type": "hasMany",
      "model": "Reminder",
      "polymorphic": "Remindable"
    }
  },
  "acls": [],
  "methods": {}
}
