{
  "name": "Quotation",
  "plural": "quotations",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Quotation": {
      "quoteID": "quoteID"
    }
  },
