{
  "name": "Account",
  "plural": "accounts",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "fullAddress": "showFullAddress",
        "name": "showFullName",
        "customers": "showAllCustomer",
        "deals": "showAllDeal",
        "industryInfo": "showIndustryInfo"
      }
    }
  },
  "scope": {
    "include": [
      "parent",
      "subaccount",
      "notes",
      "events"
    ]
  },
  "properties": {
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "customers": {
      "type": "array"
    }
  },
  "validations": [],
  "relations": {
    "parent": {
      "type": "belongsTo",
      "model": "BaseCompany",
      "foreignKey": ""
    },
    "contact": {
      "type": "embedsOne",
      "model": "BaseContact",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "subaccount": {
      "type": "hasMany",
      "model": "Account",
      "foreignKey": "parent"
    },
    "industry": {
      "type": "belongsTo",
      "model": "LeadIndustry",
      "foreignKey": ""
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "customers": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "",
      "options": {
        "nestRemoting": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
