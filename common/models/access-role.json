{
  "name": "AccessRole",
  "plural": "accessroles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "Owner": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "seePeer": {
      "type": "boolean",
      "default": false
    },
    "tier": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "accessRights": {
      "type": "hasAndBelongsToMany",
      "model": "AccessRight",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "parent": {
      "type": "belongsTo",
      "model": "AccessRole",
      "foreignKey": ""
    },
    "company": {
      "type": "belongsTo",
      "model": "BaseCompany"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "companySet",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}