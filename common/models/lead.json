{
  "name": "Lead",
  "plural": "leads",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "fullAddress": "showFullAddress",
        "name": "showFullName",
        "upcomingEvents": "showUpcoming",
        "pastEvents": "showPast"
      }
    },
    "ShowUsers": true,
    "DeleteHistory": true
  },
  "scope": {
    "include": [
      "status",
      "source",
      "industry",
      "notes"
    ]
  },
  "properties": {
    "companyName": {
      "type": "string"
    },
    "interest": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "fullAddress": {
      "type": "string"
    },
    "upcomingEvents": {
      "type": "array"
    },
    "pastEvents": {
      "type": "array"
    }
  },
  "validations": [],
  "relations": {
    "contact": {
      "type": "embedsOne",
      "model": "BaseContact",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "status": {
      "type": "belongsTo",
      "model": "LeadStatus",
      "foreignKey": ""
    },
    "source": {
      "type": "belongsTo",
      "model": "LeadSource",
      "foreignKey": ""
    },
    "industry": {
      "type": "belongsTo",
      "model": "LeadIndustry",
      "foreignKey": ""
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    }
  },
  "acls": [],
  "methods": {}
}
