{
  "name": "Lead",
  "plural": "leads",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "fullAddress": "showFullAddress",
        "name": "showFullName",
        "sourceInfo": "showSourceInfo",
        "statusInfo": "showStatusInfo",
        "industryInfo": "showIndustryInfo",
        "eventInfo": "showEventInfo"
      }
    },
    "DeleteHistory": true
  },
  "scope": {
    "include": ["notes", "followUps"]
  },
  "properties": {
    "companyName": {
      "type": "string"
    },
    "interest": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "fullAddress": {
      "type": "string"
    },
    "isConverted": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "contact": {
      "type": "embedsOne",
      "model": "BaseContact",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "status": {
      "type": "belongsTo",
      "model": "LeadStatus",
      "foreignKey": ""
    },
    "source": {
      "type": "belongsTo",
      "model": "LeadSource",
      "foreignKey": ""
    },
    "industry": {
      "type": "belongsTo",
      "model": "LeadIndustry",
      "foreignKey": ""
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "followUps": {
      "type": "hasMany",
      "model": "FollowUp",
      "polymorphic": "followupable"
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    },
    "participatedEvents": {
      "type": "hasMany",
      "model": "EventParticipant",
      "polymorphic": "participants"
    }
  },
  "acls": [],
  "methods": {}
}
