{
  "name": "Lead",
  "plural": "leads",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "fullAddress": "showFullAddress"
      }
    }
  },
  "scope": {
    "include": [
      "status",
      "source",
      "industry",
      "events"
    ]
  },
  "properties": {
    "companyName": {
      "type": "string"
    },
    "mobile": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "address_1": {
      "type": "string"
    },
    "address_2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "fullAddress": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "contact": {
      "type": "embedsOne",
      "model": "BaseContact",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "status": {
      "type": "belongsTo",
      "model": "LeadStatus",
      "foreignKey": ""
    },
    "source": {
      "type": "belongsTo",
      "model": "LeadSource",
      "foreignKey": ""
    },
    "industry": {
      "type": "belongsTo",
      "model": "LeadIndustry",
      "foreignKey": ""
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "polymorphic": "eventable"
    },
    "notes": {
      "type": "hasMany",
      "model": "Note",
      "polymorphic": "noteable"
    }
  },
  "acls": [],
  "methods": {}
}
